			<to-view-id>/public/contactus.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>public/news</from-outcome>
			<to-view-id>/public/news.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>public/news/singlenews</from-outcome>
			<to-view-id>/public/news/singlenews.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{authenticationController.login}</from-action>
			<from-outcome>member/index</from-outcome>
			<to-view-id>/member/index.xhtml</to-view-id>
		</navigation-case>
